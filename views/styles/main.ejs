body {
  font-size: 1.6rem;
  font-weight: 400;
  line-height: 1.7;
  background-color: #000;
  font-family: "Fira Code", monospace;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: 600;
}
p {
  font-weight: 400;
}

ul {
  list-style: none;
}

#root {
  height: 100%;
  width: 90%;
  background-color: #fefe;
}
